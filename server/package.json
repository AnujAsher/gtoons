{
  "name": "gtoons-server",
  "version": "0.0.1",
  "description": "gtoons remake rest and websocket server",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "start": "ts-node src/server.ts"
  },
  "author": "zak rabe",
  "license": "MIT",
  "dependencies": {
    "@types/socket.io": "^2.1.4",
    "body-parser": "^1.18.1",
    "concurrently": "^5.0.0",
    "copyfiles": "^2.1.1",
    "express": "^4.15.4",
    "helmet": "^3.21.2",
    "mysql": "^2.17.1",
    "nodemon": "^2.0.1",
    "pg": "^7.14.0",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.10",
    "socket.io": "^2.3.0",
    "typeorm": "0.2.21",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "ts-node": "3.3.0",
    "@types/express": "^4.17.2",
    "@types/node": "^8.0.29",
    "typescript": "3.3.3333"
  }
}
